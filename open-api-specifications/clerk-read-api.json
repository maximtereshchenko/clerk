{
  "openapi": "3.1.0",
  "info": {
    "title": "Clerk Read API",
    "version": "1.0.0"
  },
  "paths": {
    "/templates": {
      "get": {
        "tags": [
          "clerk read"
        ],
        "operationId": "templates",
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "Templates",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/clerkReadTemplate"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/templates/{templateId}/placeholders": {
      "get": {
        "tags": [
          "clerk read"
        ],
        "operationId": "placeholders",
        "parameters": [
          {
            "name": "userId",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "templateId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "Placeholders",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/clerkReadPlaceholders"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "clerkReadTemplate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "userId",
          "name",
          "timestamp"
        ]
      },
      "clerkReadPlaceholders": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "placeholders": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "userId",
          "placeholders",
          "timestamp"
        ]
      }
    }
  }
}
