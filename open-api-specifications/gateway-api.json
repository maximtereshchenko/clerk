{
  "openapi": "3.1.0",
  "info": {
    "title": "Gateway API",
    "version": "1.0.0"
  },
  "paths": {
    "/templates": {
      "get": {
        "tags": [
          "clerk read gateway"
        ],
        "operationId": "templates",
        "responses": {
          "200": {
            "description": "Templates",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/clerkReadGatewayTemplate"
                  }
                }
              }
            }
          },
          "default": {
            "description": "Internal server error"
          }
        }
      },
      "post": {
        "tags": [
          "clerk write gateway"
        ],
        "operationId": "createTemplate",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/clerkWriteGatewayCreateTemplateRequest"
              }
            }
          }
        },
        "responses": {
          "202": {
            "description": "Create template request accepted"
          },
          "default": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/templates/{templateId}/placeholders": {
      "get": {
        "tags": [
          "clerk read gateway"
        ],
        "operationId": "placeholders",
        "parameters": [
          {
            "name": "templateId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Placeholders",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/clerkReadGatewayPlaceholders"
                }
              }
            }
          },
          "default": {
            "description": "Internal server error"
          }
        }
      }
    },
    "/documents": {
      "get": {
        "tags": [
          "clerk read gateway"
        ],
        "operationId": "documents",
        "responses": {
          "200": {
            "description": "Documents",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/clerkReadGatewayDocument"
                  }
                }
              }
            }
          },
          "default": {
            "description": "Internal server error"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "clerkReadGatewayTemplate": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "userId",
          "name",
          "timestamp"
        ]
      },
      "clerkReadGatewayPlaceholders": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "placeholders": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "id",
          "userId",
          "placeholders",
          "timestamp"
        ]
      },
      "clerkReadGatewayDocument": {
        "type": "object",
        "properties": {
          "fileId": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "timeToLive": {
            "type": "string",
            "format": "date-time"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": [
          "fileId",
          "userId",
          "timeToLive",
          "timestamp"
        ]
      },
      "clerkWriteGatewayCreateTemplateRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "fileId": {
            "type": "string",
            "format": "uuid"
          },
          "name": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "fileId",
          "name"
        ]
      }
    }
  }
}
